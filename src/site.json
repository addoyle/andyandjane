  "venues": [
    {
      "name": "Rawlings Conservatory",
      "img": "conservatory1.jpg",
      "date": "2017-07-14",
      "desc": "The Howard Peter Rawlings Conservatory and Botanical Gardens opened in Druid Hill Park in 1888 and is one of the oldest Victorian-style conservatories still standing in the United States. <p><em>Click below for directions.</em></p>",
      "href": "https://www.google.com/maps/place/Rawlings+Conservatory/@39.3185704,-76.6479233,17z/data=!3m1!4b1!4m5!3m4!1s0x89c81b33c0e81c41:0x993d3cb5f38f5105!8m2!3d39.3185704!4d-76.6457346",
      "icon": "map-marker"
    },
    {
      "name": "Gardens",
      "img": "conservatory2.jpg",
      "date": "2017-07-14",
      "desc": "Our guests will view beautiful living plants from around the world in the Palm House, the Orchid Room, the Mediterranean Garden, the Desert Garden, and the Tropical Garden.",
      "icon": "leaf"
    },
    {
      "name": "Outdoor Activities",
      "img": "conservatory3.jpg",
      "date": "2017-07-14",
      "desc": "Tour the classical Rose Garden, featuring a multi-faceted sundial crafted in 1890, and get down with your bad self on a tented outdoor dance-floor.",
      "icon": "sun-o"
    }
  ],
